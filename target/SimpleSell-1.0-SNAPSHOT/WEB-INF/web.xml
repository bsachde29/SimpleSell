<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>MyServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/MyServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SignUp</servlet-name>
        <servlet-class>SignUp</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SignUp</servlet-name>
        <url-pattern>/SignUp</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>ModifyAccountDetails</servlet-name>
        <servlet-class>ModifyAccountDetails</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ModifyAccountDetails</servlet-name>
        <url-pattern>/ModifyAccountDetails</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Inventory</servlet-name>
        <servlet-class>Inventory</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Inventory</servlet-name>
        <url-pattern>/Inventory</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FinancialReports</servlet-name>
        <servlet-class>FinancialReports</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>FinancialReports</servlet-name>
        <url-pattern>/FinancialReports</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ModifyProducts</servlet-name>
        <servlet-class>ModifyProducts</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ModifyProducts</servlet-name>
        <url-pattern>/ModifyProducts</url-pattern>
    </servlet-mapping>



</web-app>